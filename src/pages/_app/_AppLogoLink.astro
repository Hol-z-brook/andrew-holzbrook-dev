---
import { makeSherpa } from "../_sherpa/_Sherpa";
import AppLink from "./_AppLink.astro";
import AppLogo from "./_AppLogo.astro";

interface Props {
  class?: string;
}

const {
  class: className,
} = Astro.props;

const sherpa = makeSherpa();
---

<AppLink href={sherpa.home()} class={className}>
  <AppLogo />
</AppLink>
