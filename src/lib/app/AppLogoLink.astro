---
import { makeHomeSherpa } from '../../pages/_/home.sherpa';
import AppLink from './AppLink.astro';
import AppLogo from './AppLogo.astro';

interface Props {
  class?: string;
}

const { class: className } = Astro.props;

const sherpa = makeHomeSherpa();
---

<AppLink href={sherpa.home({})} class={className}>
  <AppLogo />
</AppLink>
